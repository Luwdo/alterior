{
	"compilerOptions": {
		"experimentalDecorators": true,
		"target": "es5",
		"sourceMap": true,
		"outDir": "./dist",
		"module": "commonjs",
		"moduleResolution": "node",
		"preserveSymlinks": true,
		"typeRoots": [
			"./node_modules/@types"
		],
		"lib": [
			"es2016",
			"dom"
		],
		"declaration": true,
		"emitDecoratorMetadata": true,
		"downlevelIteration": true
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}